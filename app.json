{
  "expo": {
    "name": "BrandPeek",
    "slug": "brandpeek",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0F19"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brandpeek.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo.png",
        "backgroundColor": "#0B0F19"
      },
      "package": "com.brandpeek.app"
    },
    "web": {
      "favicon": "./assets/Logo.png"
    },
    "plugins": [
      "expo-font",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "4028a5ca-86c5-45bf-8493-3ff7960bbba0"
      }
    },
    "owner": "dheerxj"
  }
}
